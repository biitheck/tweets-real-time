        "start": "ng serve --port 8000",
        "start:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng serve --open",
        "start:lan": "ng serve --host 0.0.0.0 --port 8000",
        "build": "ng build",
        "build:prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --aot",
        "build:prod:mem": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --aot",
        "test": "ng test",
