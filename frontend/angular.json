{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "tweets-real-time-frontend": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "../backend/src/www",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "node_modules/material-design-icons/iconfont/material-icons.css",
                            "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
                            "src/styles.scss",
                            "node_modules/ngx-toastr/toastr.css"
                        ],
                        "scripts": [],
                        "showCircularDependencies": false,
                        "allowedCommonJsDependencies": [
                            "lodash",
                            "zen-observable"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/",
                                "src/scss"
                            ]
                        }
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "6mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "1028kb"
                                }
                            ]
                        }
                    }
                },
                "ec": {
                    "budgets": [
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "6kb"
                        }
                    ],
                    "sourceMap": true,
                    "extractCss": true
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "tweets-real-time-frontend:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "tweets-real-time-frontend:build:production"
                        },
                        "ec": {
                            "browserTarget": "tweets-real-time-frontend:build:ec"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "tweets-real-time-frontend:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/",
                                "src/scss"
                            ]
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/",
                                "src/scss"
                            ]
                        }
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "tweets-real-time-frontend:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "tweets-real-time-frontend:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "tweets-real-time-frontend"
}